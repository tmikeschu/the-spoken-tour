    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.1"
  },
  "dependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.24.0",
    "prop-types": "^15.6.0",
